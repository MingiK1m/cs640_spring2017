h1 java Iperfer -s -p 9090 > h1.txt &
h5 java Iperfer -s -p 9090 > h5.txt &

h4 java Iperfer -c -h "10.0.0.1" -p 9090 -t 20 > h4.txt &
h6 java Iperfer -c -h "10.0.0.5" -p 9090 -t 20 > h6.txt &

